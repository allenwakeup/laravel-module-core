{
    "name": "goodcatch/laravel-module-core",
    "description": "goodcatch laravel module - core, the most practically and basic module for modules",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allen",
            "email": "ali@goodcatch.cn",
            "homepage": "http://note.goodcatch.cn/it/dev-laravel/laravel-modules/laravel-module-core",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "ext-json": "*",
        "laravel/framework": "^8.65",
        "laravel/helpers": "^1.4",
        "goodcatch/laravel-module-base": "dev-master",
        "goodcatch/admin": "dev-master",
        "elendev/composer-push": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\Core\\": "src"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "secure-http": false,
        "allow-plugins": {
            "elendev/composer-push": true
        }
    },
    "version": "dev-master"
}
